---
layout: null
---
[
  {% for paper in site.papers %}
    {
      "title": "{{ paper.paper_title | escape }}",
      "url": "{{ paper.url | absolute_url }}",
      "authors": "{{ paper.paper_authors | strip_html | strip_newlines | escape }}",
      "year": "{{ paper.published }}",
      "content": "{{ paper.content | strip_html | strip_newlines | escape }}"
    } {% unless forloop.last %},{% endunless %}
  {% endfor %}
]
